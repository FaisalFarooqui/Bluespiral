    <!-- Shopping Cart-->
    <div class="table-responsive shopping-cart">
        <table class="table">
            <thead>

                <tr>
                    <th>Product Name</th>
                    <th class="text-center">Quantity</th>
                    <th class="text-center">Subtotal</th>
                    <th class="text-center">Discount</th>
                    <th class="text-center">Total</th>
                    <th class="text-center"><a class="btn btn-sm btn-outline-danger" href="#">Clear Cart</a></th>
                </tr>
            </thead>
            <tbody>
                @if (ViewBag.UserCart != null)
                {
                    foreach (var item in ViewBag.UserCart)
                    {
                <tr>
                    <td>
                        <input type="hidden" id="hdnid" value="@item.Id" />
                        <div class="product-item">
                            <a class="product-thumb" href="SingleProduct/" @item.ProductId><img src=@item.ImgUrl alt="Product"></a>
                            <div class="product-info">
                                <h4 class="product-title"><a href="SingleProduct/" @item.ProductId>@item.ProductName</a></h4><span><em>Size:</em> 10.5</span><span><em>Color:</em> Dark Blue</span>
                            </div>
                        </div>
                    </td>
                    <td class="text-center">
                        <div class="count-input">
                            <input type="number" class="form-control" id="quantity" inputmode="numeric" value="@item.Quantity" onblur="updatetocart();" />
                        </div>
                    </td>
                    <td class="text-center text-lg text-medium">Rs-@item.SubPrice</td>
                    <td class="text-center text-lg text-medium">Rs-@item.Discount</td>
                    <td class="text-center text-lg text-medium">Rs-@item.Price</td>
                    <td class="text-center"><a class="remove-from-cart" href="/ShopOwner/ShopOwner/RemoveCartOrder/@item.Id" ><i class="icon-cross"></i></a></td>
                </tr>
                    }
                }
